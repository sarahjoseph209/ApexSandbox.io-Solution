public Integer fibonacci(Integer n) {
    if (n == 1 || n== 2))
        return 1;
    Integer currentValue = 1;
    Integer lastValue = 1;
    Integer temp = 0;
    for (Integer i  = 3; i <= n; i++) {
        currentValue += temp;
        temp = lastValue;
        lastValue = currentValue;
    }
    return currentValue;
}